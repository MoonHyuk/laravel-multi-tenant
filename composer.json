{
    "name": "ejlab/laravel-multi-tenant",
    "version" : "0.2.2",
    "license": "MIT",
    "description": "Multi tenant package for Laravel Framework",
    "authors": [
        {
            "name": "Eunjun Park",
            "email": "ej@ej-lab.com"
        }
    ],
    "require": {
        "php": "^7.1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "orchestra/testbench": "^3.6",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "EJLab\\Laravel\\MultiTenant\\": "src/"
        },
        "classmap": [
            "tests"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "EJLab\\Laravel\\MultiTenant\\Providers\\MultiTenantServiceProvider",
                "EJLab\\Laravel\\MultiTenant\\Providers\\PassportServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
